{
  "name": "Behave",
  "description": "Client-side user-behaviour recognition.",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "md5.min.js",
        "jquery.min.js",
        "math.min.js",
        "main.js"
      ]
    }
  ],
  "permissions": [
        "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}